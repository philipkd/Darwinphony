		public function play(note:uint):void {
			var tps:Number = 8;
			var tone:Sfx = tones[note][int(timer * tps)];
			if (!tone.playing)
				tone.play();
		}
		
		public function tick(elapsed:Number):void {
			timer += elapsed;
			if (timer > 2)
				timer -= 2;			
		}
		
